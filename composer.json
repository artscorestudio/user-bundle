        "symfony/security-bundle": "~2.3|~3.0",
        "symfony/twig-bundle": "~2.3|~3.0",
        "friendsofsymfony/user-bundle": "~2.0@dev",
        "artscorestudio/core-bundle": "^1.0",
        "artscorestudio/layout-bundle": "^1.0"
